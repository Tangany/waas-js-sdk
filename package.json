{
  "name": "@tangany/waas-js-sdk",
  "version": "0.0.1-alpha.1",
  "description": "JavaScript SDK for Tangany Wallet as a Service API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.5"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "mocha src/**/*.spec.ts",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "build": "rimraf dist/* && tsc -p tsconfig.json",
    "prepare": "npm run lint:fix && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "node postversion.js && git add * && git push && git push --tags",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tangany/waas-js-sdk.git"
  },
  "keywords": [
    "ethereum",
    "erc20",
    "wallet",
    "wallet as a service",
    "waas",
    "tangany",
    "blockchain",
    "api",
    "private key",
    "token"
  ],
  "author": "Tangany <developers@tangany.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Tangany/waas-js-sdk/issues"
  },
  "homepage": "https://tagany.com",
  "devDependencies": {
    "@types/debug": "^4.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^5.2.6",
    "@types/moxios": "^0.4.8",
    "@types/node": "^11.12.2",
    "@types/sinon": "^7.0.11",
    "backend-tslint-rules": "^1.12.1",
    "dotenv": "^7.0.0",
    "mocha": "^6.1.3",
    "moxios": "^0.4.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "swagmock": "^1.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1"
  }
}
