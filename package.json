{
  "name": "@tangany/waas-js-sdk",
  "version": "1.0.0-beta.0",
  "description": "JavaScript SDK for Tangany Wallet as a Service API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.5"
  },
  "files": [
    "dist/**/*",
    "readme.md"
  ],
  "scripts": {
    "test": "npm run build && mocha test/*.e2e.js",
    "test:unit": "mocha src/**/*.spec.ts",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "build": "rimraf dist/* && tsc -p tsconfig.json",
    "prepare": "npm run lint:fix && npm run build",
    "prepublishOnly": "npm run test:unit && npm test && npm run lint",
    "badge:npm": "node scripts/get-npm-version-badge.js",
    "badge:sdk": "node scripts/get-sdk-version-badge.js",
    "postversion": "npm run postversion:build && git add * && git push && git push --tags",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tangany/waas-js-sdk.git"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "bitcoin",
    "api",
    "erc20",
    "token",
    "wallet",
    "wallet as a service",
    "waas",
    "tangany"
  ],
  "author": "Tangany <developers@tangany.com> (https://tangany.com)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Tangany/waas-js-sdk/issues"
  },
  "homepage": "https://github.com/Tangany/waas-js-sdk#readme",
  "devDependencies": {
    "@tangany/tslint-config": "^1.1.1",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^5.2.7",
    "@types/moxios": "^0.4.8",
    "@types/node": "^12.6.9",
    "@types/sinon": "^7.0.13",
    "dotenv": "^8.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.4.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "moxios": "^0.4.0",
    "typeforce": "^1.18.0"
  }
}
